// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+shift+cmd+r",
        "command": "workbench.action.debug.run"
    },
    {
        "key": "ctrl+f5",
        "command": "-workbench.action.debug.run"
    },
    {
        "key": "ctrl+cmd+r",
        "command": "workbench.action.debug.start",
        "when": "!inDebugMode"
    },
    {
        "key": "f5",
        "command": "-workbench.action.debug.start",
        "when": "!inDebugMode"
    }
]